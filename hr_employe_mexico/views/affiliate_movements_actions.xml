        <field name="tag">employee_report</field>
        <field name="context" eval="{'model': 'report.affiliate.movements', 'type': '08-ALTA'}"/>
    </record>

    <!--   Type Salary    -->
    <record id="action_affiliate_movements_salary" model="ir.actions.client">
